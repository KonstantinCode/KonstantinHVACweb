@{
	ViewData["Title"] = "Sākums";
}

<!-- Image Carousel -->

<div id="carousel" class="carousel slide py-0" data-ride="carousel" data-interval="6500" data-keyboard="true">

	<!-- Carousel Content -->

	<ol class="carousel-indicators">
		<li data-target="#carousel" data-slide-to="0" class="active"></li>
		<li data-target="#carousel" data-slide-to="1"></li>
		<li data-target="#carousel" data-slide-to="2"></li>
		<li data-target="#carousel" data-slide-to="3"></li>
		<li data-target="#carousel" data-slide-to="4"></li>
		<li data-target="#carousel" data-slide-to="5"></li>
	</ol>

	<div class="carousel-inner">

		<div class="carousel-item active">
			<img src="~/content/images/carousel/1.jpg" alt="" class="w-100" />
			<div class="carousel-caption">
				<div class="container">
					<div class="row justify-content-center">
						<div class="col-8 bg-custom d-none d-lg-block py-3 px-0 ">
							<h1>AVK Dizains</h1>
							<div class="border-top border-primary w-50 mx-auto my-3"></div>
							<h3 class="pb-3">piedāvā AVK projektēšanas un konsultāciju pakalpojumus</h3>
							<a class="btn btn-primary btn-sm ml-2" asp-controller="Home" asp-action="Services" role="button">Iepazīt...</a>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="carousel-item">
			<img src="~/content/images/carousel/2.jpg" alt="" class="w-100" />
			<div class="carousel-caption">
				<div class="container">
					<div class="row justify-content-end text-right">
						<div class="col-7 bg-custom d-none d-lg-block py-3 px-0 pr-3">
							<h3 class="pb-3">
								Visu veidu AVK progresivi aprēķini<br />
								Energoefektīvi risinājumi
							</h3>
							<a class="btn btn-primary btn-sm ml-2" asp-controller="Home" asp-action="Services" role="button">Iepazīt...</a>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="carousel-item">
			<img src="~/content/images/carousel/3.jpg" alt="" class="w-100" />
			<div class="carousel-caption">
				<div class="containers">
					<div class="row justify-content-start text-left">
						<div class="col-7 bg-custom d-none d-lg-block py-3 px-0 pl-3">

							<h3 class="pb-3">Apkures un dzesēšanas sistēmu projektēšana</h3>
							<p class="lead"></p>
							<a class="btn btn-primary btn-sm ml-2" asp-controller="Home" asp-action="Services" role="button">Iepazīt...</a>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="carousel-item">
			<img src="~/content/images/carousel/4.jpg" alt="" class="w-100" />
			<div class="carousel-caption">
				<div class="containers">
					<div class="row justify-content-center text-center">
						<div class="col-7 bg-custom d-none d-lg-block py-3 px-0 pl-3">

							<h3 class="pb-3">Gaisa kondicionēšanas sistēmu projektēšana</h3>
							<p class="lead"></p>
							<a class="btn btn-primary btn-sm ml-2" asp-controller="Home" asp-action="Services" role="button">Iepazīt...</a>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="carousel-item">
			<img src="~/content/images/carousel/6.jpg" alt="" class="w-100" />
			<div class="carousel-caption">
				<div class="containers">
					<div class="row justify-content-start text-left">
						<div class="col-7 bg-custom d-none d-lg-block py-3 px-0 pl-3">

							<h3 class="pb-3">Būvniecības informācijas modelēšana (BIM)</h3>
							<p class="lead"></p>
							<a class="btn btn-primary btn-sm ml-2" asp-controller="Home" asp-action="Services" role="button">Iepazīt...</a>
